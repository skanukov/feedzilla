<div class="container container-content">
  <div class="row">
    <div class="col-xs-12">
      <p>
        <strong>Название:</strong>
        <%= @feed.title %>
      </p>

      <p>
        <strong>Адрес:</strong>
        <%= @feed.link %>
      </p>

      <p>
        <strong>Описание:</strong>
        <%= @feed.description %>
      </p>

      <div class="form-footer-links">
        <%= link_to 'Редактировать', edit_admin_feed_path(@feed) %> |
        <%= link_to 'Назад', admin_feeds_path %>
      </div>
    </div>
  </div>
</div>
